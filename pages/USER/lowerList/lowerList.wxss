
.color_96 {
  color: #969696;
}
.much {
  padding: 60rpx 0 50rpx;
  font-size: 36rpx;
  text-align: center;
}
.item {
  padding: 20rpx 25rpx;
  border-bottom: 1rpx solid #EFEFF4;
  background-color: #fff;
}
.item:last-child {
  border-bottom: 0;
}
.avatar {
  overflow: hidden;
  display: block;
  width: 50rpx;
  height: 50rpx;
  margin-right: 20rpx;
  border-radius: 50%;
}
.nickname {
  margin-top: 6rpx;
  font-size: 24rpx;
}